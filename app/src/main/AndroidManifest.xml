<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.anthony.timelapscontroller">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/time_lapse"
        android:label="@string/app_name"
        android:roundIcon="@drawable/time_lapse"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".VideoActivity"></activity>
        <activity android:name=".CameraActivity" />
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".NewExecutionActivity" />
        <activity android:name=".ConsultActivity" />
        <activity android:name=".ModifyActivity" />
        <activity
            android:name=".PhotoActivity"
            android:label="@string/title_activity_photo"
            android:theme="@style/AppTheme.NoActionBar" />
    </application>

</manifest>